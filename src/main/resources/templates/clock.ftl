<html lang="zh">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<title>js动态炫酷数字时钟</title>


</head>
<body>


<div>
	<link rel="stylesheet" type="text/css"
		href="${request.contextPath}/css/normalize.css" />
	<link rel="stylesheet" type="text/css" href="${request.contextPath}/css/demo(1).css" />
	<link rel="stylesheet" type="text/css" href="${request.contextPath}/css/style.css" />
	<div class="clock">
		<div class="digit hours" data-value="2">
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
		</div>

		<div class="digit hours" data-value="3">
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
		</div>

		<div class="separator"></div>

		<div class="digit minutes" data-value="2">
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
		</div>

		<div class="digit minutes" data-value="5">
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
		</div>

		<div class="separator"></div>

		<div class="digit seconds" data-value="3">
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment"></div>
			<div class="segment on"></div>
		</div>

		<div class="digit seconds" data-value="5">
			<div class="segment"></div>
			<div class="segment on"></div>
			<div class="segment on"></div>
			<div class="segment"></div>
			<div class="segment"></div>
			<div class="segment"></div>
			<div class="segment"></div>
		</div>
	</div>
	<script src="${request.contextPath}/js/digital-clock.js.下载" type="text/javascript"></script>
</div>

</body>
</html>